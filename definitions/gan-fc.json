{
  "experiment-options": {
    "workflow": "rsm.workflows.gan_workflow",
    "dataset": "pagi.datasets.mnist_dataset",
    "component": "rsm.components.gan_component",
    "batches": 50000,

    "hparams_override": {
      "batch_size": 128,
      "conditional": true,

      "generator_num_layers": 1,
      "generator_filters": [128],
      "generator_filters_field_width": [1],
      "generator_filters_field_height": [1],
      "generator_filters_field_stride": [1],
      "generator_nonlinearity": ["relu"],
      "generator_loss_mse_lambda": 0.0,
      "generator_loss_adv_lambda": 1.0,
      "generator_learning_rate": 0.0005,
      "generator_input_size": [1, 1, 100],
      "generator_input_nonlinearity": "relu",
      "generator_output_nonlinearity": "sigmoid",

      "discriminator_num_layers": 1,
      "discriminator_filters": [128],
      "discriminator_filters_field_width": [28],
      "discriminator_filters_field_height": [28],
      "discriminator_filters_field_stride": [28],
      "discriminator_nonlinearity": ["leaky_relu"],
      "discriminator_learning_rate": 0.0005,
      "discriminator_input_size": [28, 28, 1],
      "discriminator_input_nonlinearity": "relu",
      "discriminator_output_nonlinearity": "sigmoid"
    }
  },
  "workflow-options": {
    "train": true,
    "evaluate": false
  }
}
