{
  "experiment-options": {
    "workflow": "rsm.workflows.gan_workflow",
    "dataset": "pagi.datasets.mnist_dataset",
    "component": "rsm.components.gan_component",
    "batches": 1000,

    "hparams_override": {
      "batch_size": 128,

      "generator_num_layers": 2,
      "generator_filters": [128, 256],
      "generator_filters_field_width": [1, 1],
      "generator_filters_field_height": [1, 1],
      "generator_filters_field_stride": [1, 1],
      "generator_nonlinearity": ["relu", "relu"],
      "generator_loss_lambda": 0.9,
      "generator_learning_rate": 0.001,

      "discriminator_num_layers": 2,
      "discriminator_filters": [32, 64],
      "discriminator_filters_field_width": [6, 6],
      "discriminator_filters_field_height": [6, 6],
      "discriminator_filters_field_stride": [3, 3],
      "discriminator_nonlinearity": ["relu", "relu"],
      "discriminator_learning_rate": 0.001

    }
  },
  "workflow-options": {
    "train": true,
    "evaluate": false
  }
}
