{
  "experiment-options": {
    "workflow": "rsm.workflows.composite_gan_workflow",
    "dataset": "pagi.datasets.mnist_dataset",
    "component": "rsm.components.composite_gan_stack",
    "batches": 5000,

    "hparams_override": {
      "batch_size": 128,


      "ae_stack_batch_size": 128,
      "ae_stack_summarize_decoding": [true],


      "gan_batch_size": 128,

      "gan_generator_num_layers": 1,
      "gan_generator_filters": [128],
      "gan_generator_filters_field_width": [10],
      "gan_generator_filters_field_height": [10],
      "gan_generator_filters_field_stride": [10],
      "gan_generator_nonlinearity": ["leaky_relu"],
      "gan_generator_loss_mse_lambda": 1.0,
      "gan_generator_loss_adv_lambda": 0.9,
      "gan_generator_learning_rate": 0.0005,
      "gan_generator_output_nonlinearity": "tanh",

      "gan_discriminator_num_layers": 1,
      "gan_discriminator_filters": [128],
      "gan_discriminator_filters_field_width": [28],
      "gan_discriminator_filters_field_height": [28],
      "gan_discriminator_filters_field_stride": [28],
      "gan_discriminator_nonlinearity": ["leaky_relu"],
      "gan_discriminator_learning_rate": 0.0005,
      "gan_discriminator_output_nonlinearity": "sigmoid",
      "gan_discriminator_input_noise": true

    }
  },
  "workflow-options": {
    "train": true,
    "evaluate": false,
    "ae_train_steps": 2000
  }
}
